{# def d, level #}
<ITable>
  <ITableField d={d.award_title} level={level}/>
  <ITableField d={d.funding_program} level={level}/>
  <ITableField d={d.iri} level={level}/>
  <ITableField d={d.local_identifier} level={level}/>
</ITable>
{% for item in array(d.funders) %}
  <ITableSection d={item} level={level} >
    <Funder d={item}  level={level}/>
  </ITableSection>
{% endfor %}