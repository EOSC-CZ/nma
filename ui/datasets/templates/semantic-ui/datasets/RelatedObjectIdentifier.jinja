{# def d, level #}
<ITable>
  <ITableField d={d.identifier} level={level}/>
  <ITableField d={d.iri} level={level}/>
  <ITableField d={d.relation_type} level={level}/>
  <ITableField d={d.title} level={level}/>
  <ITableField d={d.type} level={level}/>
</ITable>
{% for item in array(d.qualified_relations) %}
  <ITableSection d={item} level={level} >
    <QualifiedRelation d={item}  level={level}/>
  </ITableSection>
{% endfor %}
{% for item in array(d.time_references) %}
  <ITableSection d={item} level={level} >
    <TimeReference d={item}  level={level}/>
  </ITableSection>
{% endfor %}