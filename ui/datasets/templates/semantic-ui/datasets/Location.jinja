{# def d, level #}
<ITable>
  <ITableField d={d.dataset_relations} level={level}>
    <ITableArrayValue value={d.dataset_relations} />
  </ITableField>
  <ITableField d={d.iri} level={level}/>
  <ITableField d={d.location_names} level={level}>
    <ITableArrayValue value={d.location_names} />
  </ITableField>
</ITable>
{% for item in array(d.related_object_identifiers) %}
  <ITableSection d={item} level={level} >
    <RelatedObjectIdentifier d={item}  level={level}/>
  </ITableSection>
{% endfor %}