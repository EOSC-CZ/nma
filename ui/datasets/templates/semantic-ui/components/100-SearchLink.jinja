{#def className, search_link, searchFacet, data #}
{% set data_dict = data | to_dict %}
{% set href = search_link ~ "?q=&f=" ~ searchFacet ~ ":" ~ data_dict.value %}

{% set data_dict = {
    "href": href,
    "title": data_dict.title,
    "label": data_dict.label
} %}

<a class="ui {{className}}" href="{{data_dict.href | e}}" aria-label="{{data_dict.title}}" title="{{data_dict.title}}">
  <span class="{{className}} label">
    {{content if content else data_dict.label if data_dict.label else data_dict.title}}
  </span>
</a>