{#def className=None, search_link, searchFacet, data, searchFacetValue=None, hasTooltip=True #}
{% set data_dict = data | to_dict %}
{% set href = search_link ~ "?q=&f=" ~ searchFacet ~ ":" ~ (searchFacetValue|urlencode if searchFacetValue else data_dict.value|urlencode) %}

{% set url_data = {
    "href": href,
    "title": data_dict.title,
    "label": data_dict.label,
    "aria": data_dict.aria
} %}

<URL className={className} data = {url_data} hasTooltip={hasTooltip}></URL>